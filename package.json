    "@types/babel__preset-env": "^7.9.2",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/eslint": "8.4.2",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "4.14.182",
    "@types/prettier": "^2.6.0",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^18.0.8",
