{
  "name": "md-to-tid",
  "version": "0.0.4",
  "description": "Transform markdown to tiddlywiki5 wikitext syntax.",
  "main": "dist/index.mjs",
  "type": "module",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "test": "npm run build:js && NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.json",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build:demo": "rollup -c rollup.config.demo.js && cp demo/index.html public-dist/index.html",
    "dev:demo": "mkdir -p public-dist/ && cp demo/index.html public-dist/index.html && rollup -c rollup.config.demo.development.js --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rimraf dist public-dist",
    "installType": "typesync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/md-to-tid.git"
  },
  "files": [
    "dist/index.mjs",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "keywords": [
    "generator",
    "template"
  ],
  "author": "lin onetwo",
  "license": " GPL-3.0",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/md-to-tid/issues"
  },
  "homepage": "https://github.com/tiddly-gittly/md-to-tid#readme",
  "dependencies": {
    "lodash": "4.17.21",
    "longest-streak": "^3.0.1",
    "remark-parse": "^10.0.1",
    "type-fest": "^2.12.2",
    "unified": "^10.1.2",
    "vfile": "^5.3.2",
    "zwitch": "^2.0.2"
  },
  "devDependencies": {
    "@blueprintjs/core": "^4.3.0",
    "@fluentui/react": "^8.67.2",
    "@rjsf/core": "^4.1.1",
    "@rjsf/fluent-ui": "^4.1.1",
    "beautiful-react-hooks": "^1.2.0",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
